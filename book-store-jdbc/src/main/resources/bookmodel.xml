<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="Book1" class="com.alfianhid.model.Book">
        <property name="title" value="Dunia Sophie"/>
        <property name="description" value="Sebuah novel filsafat"/>
        <property name="publisher" value="Mizan Publishing"/>
        <property name="language" value="Bahasa Indonesia"/>
        <property name="year" value="1991"/>
        <property name="page" value="800"/>
        <property name="stock" value="50"/>
        <property name="price" value="135000"/>
    </bean>
    <bean id="Book2" class="com.alfianhid.model.Book">
        <property name="title" value="Kami (Bukan) Sarjana Kertas"/>
        <property name="description" value="Sebuah novel ironi nan retorik bagi mahasiswa"/>
        <property name="publisher" value="Bukune Publishing"/>
        <property name="language" value="Bahasa Indonesia"/>
        <property name="year" value="2019"/>
        <property name="page" value="372"/>
        <property name="stock" value="100"/>
        <property name="price" value="88000"/>
    </bean>
</beans>